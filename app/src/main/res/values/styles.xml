<?xml version="1.0" encoding="utf-8"?>
<resources>
    
    <style name="AppTheme" parent="@android:style/Theme.Holo.Light" />
	<style name="Transparent" parent="@android:style/Theme.Holo.Light.NoActionBar">
        <!--不设置activity进入和退出动画样式-->
        <item name="android:windowAnimationStyle">@null</item>
        <!--设置窗口的背景为透明，设置透明背景必须要设置此项-->
        <item name="android:windowBackground">@android:color/transparent</item>
        <!--设置窗口的背景是否为半透明，设置透明背景必须要设置此项-->
        <item name="android:windowIsTranslucent">true</item>
        <!--设置状态栏的背景为半透明-->
        <item name="android:windowTranslucentStatus">false</item>
    </style>
	<style name="WarpLinearLayoutDefault">
        <item name="grivate">left</item>
        <item name="horizontal_Space">20dp</item>
        <item name="vertical_Space">20dp</item>
        <item name="isFull">false</item>
	</style>
</resources>
